{
  "name": "@ovh-api/api",
  "description": "Ovh api connector implementation",
  "version": "4.0.0",
  "type": "module",
  "keywords": [
    "ovh",
    "ovhCloud",
    "api",
    "typing",
    "typescript",
    "REST",
    "api.ovh.com"
  ],
  "main": "dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "license": "MIT",
  "author": "Uriel Chemouni <uchemouni@gmail.com> (https://uriel.deno.dev/)",
  "dependencies": {
    "@ovh-api/common": "4.0.0",
    "open": "^8.4.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "ava": "^5.1.0",
    "chai": "^4.3.7",
    "mocha": "^10.2.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "bugs": "https://github.com/UrielCh/api-ovh-node/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UrielCh/api-ovh-node.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "ncu": "npx npm-check-updates -i",
    "clean": "rimraf dist",
    "build": "tsc --pretty --project . && tsc --pretty --project tsconfig-cjs.json",
    "prepare": "npm run build",
    "build:watch": "tsc --pretty --project . --watch",
    "test": "ava"
  },
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm",
      "--no-warnings"
    ]
  },
  "gitHead": "b9fec53f25067fa01d341adc086a63ef2c2a5f62"
}